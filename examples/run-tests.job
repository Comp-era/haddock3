#!/bin/bash
#SBATCH --nodes=1
#SBATCH --tasks-per-node=40
#SBATCH -J hd3tests
#SBATCH -p short
#SBATCH -t 0-2:00

# make sure anaconda is activated
source $HOME/miniconda3/bin/activate
conda activate haddock3

# go to the example directory
cd $HOME/haddock3/examples

# execute
python run_tests.py -b
